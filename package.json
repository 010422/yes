{
  "name": "YesPlayMusic",
  "version": "0.4.9",
  "description": "A simple music player",
  "main": "src/main.js",
  "scripts": {
  "start": "electron .",
  "build": "electron-builder",  
  "build:macos": "electron-builder --mac --x64"  
},
  "dependencies": {
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "vue-gtag": "^1.5.0",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "electron": "^24.8.8",
    "electron-builder": "^24.13.3",
    "@vue/cli-service": "^5.0.0",
    "vue-loader": "^15.9.8",
    "webpack": "^5.96.2",
    "webpack-cli": "^4.0.0",
    "babel-loader": "^8.1.0",
    "esbuild-loader": "^2.9.0"
  },
  "build": {
    "appId": "com.yourname.yesplaymusic",
    "mac": {
      "target": [
        "dmg"
      ],
      "defaultArch": "x64"
      "arch": "x64"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
