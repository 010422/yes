{
  "name": "YesPlayMusic",
  "version": "0.4.9",
  "description": "A simple music player",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --mode production",
    "electron-build": "electron-builder"
  },
  "dependencies": {
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "electron": "^24.8.8",
    "electron-builder": "^24.13.3",
    "webpack": "^5.77.0",
    "webpack-cli": "^4.9.2",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "babel-loader": "^8.2.5",
    "vue-style-loader": "^4.1.3",
    "css-loader": "^6.7.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "sass": "^1.49.0"
  },
  "build": {
    "appId": "com.yourname.yesplaymusic",
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
