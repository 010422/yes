{
  "name": "YesPlayMusic",
  "version": "0.4.9",
  "description": "Your description here",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^24.8.8",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.yourapp.id",
    "productName": "YesPlayMusic",
    "files": [
      "dist/**/*",
      "build/icons/**/*",
      "src/**/*"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/icons/icon.icns",
      "dmg": {
        "icon": "build/icons/icon.icns",
        "iconSize": 100,
        "window": {
          "width": 500,
          "height": 350
        }
      }
    }
  }
}
