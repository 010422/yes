{
  "name": "YesPlayMusic",
  "version": "0.4.9",
  "description": "A simple music player",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "vue": "^2.6.14",  
    "vue-gtag": "^1.0.0",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "electron": "^24.8.8",
    "electron-builder": "^24.13.3",
    "vue-cli-service": "^4.5.15"
  },
  "build": {
    "appId": "com.yourname.yesplaymusic",
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/icons/icon.icns",
      "category": "public.app-category.music",
      "darkModeSupport": true
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons/icon.png"
    }
  }
}
